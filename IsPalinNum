class UserMainCode
{
    public int isPalinNum(int input1){

        int n = input1;
        int rev = 0;

        while (n > 0) {
            int rem = n % 10;
            rev = rev * 10 + rem;
            n = n / 10;
        }

        if (rev == input1)
            return 2;      // palindrome
        else
            return 1;      // not palindrome
    }
}
